ket) {\r\n            setCookie(\"basket\", id);\r\n            return true;\r\n        }\r\n\r\n        let basket = cookies.basket;\r\n        basket = basket + \" \" + id;\r\n        setCookie(\"basket\", basket);\r\n        let arr = basket.split(\" \");\r\n    }\r\n\r\n    \r\n\r\n\r\n    return (\r\n        <div className=\"p-3 col-4\">\r\n            <Card>\r\n                <Card.Img variant=\"top\" className=\"p-5\" src={product.imageURL} />\r\n                <Card.Body>\r\n                    <Card.Title>{product.name}</Card.Title>\r\n                </Card.Body>\r\n                <Card.Footer className=\"text-left\" style={{cursor:\"pointer\"}}>\r\n                    <small className=\"text-success\" onClick={() => addCard(product.name, product.id) } >+ Add to card</small>\r\n                </Card.Footer>\r\n                <Notifications  options={{zIndex: 200, top: '50px'}} />\r\n            </Card>\r\n        </div>\r\n    );\r\n}\r\nexport default Product;","C:\\Users\\Мадияр\\Desktop\\Projects\\react_spring-1\\src\\components\\Profile\\Profilebar.jsx",[],"C:\\Users\\Мадияр\\Desktop\\Projects\\react_spring-1\\src\\components\\Authorization\\LoginForm.jsx",[],"C:\\Users\\Мадияр\\Desktop\\Projects\\react_spring-1\\src\\components\\Home\\asidemenu\\AsideMenu.jsx",[],"C:\\Users\\Мадияр\\Desktop\\Projects\\react_spring-1\\src\\components\\General\\Loader\\Loading.jsx",[],"C:\\Users\\Мадияр\\Desktop\\Projects\\react_spring-1\\src\\components\\Products\\ProductAdd.jsx",["144"],"import React, { useState } from \"react\";\r\nimport { Button, Form, Modal } from \"react-bootstrap\";\r\nimport { useParams } from \"react-router-dom\";\r\n\r\nconst initialState = {\r\n    name: \"\",\r\n    description: \"\",\r\n    imageURL: \"\",\r\n    uriName: \"\"\r\n};\r\n\r\nfunction ProductAdd({ setData, data }) {\r\n    const [show, setShow] = useState(false);\r\n    const [product, setProduct] = useState(initialState);\r\n    const handleClose = () => setShow(false);